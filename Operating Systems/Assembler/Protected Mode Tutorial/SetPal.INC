; mit Hilfe einer Palette wird ein 8 Bit Farbindex auf einen 18 Bit RGB Wert abgebildet

SetPal  proc
    push ds
    push cs
    pop ds
    mov dx,03c8h						; Kanal-Nummer
    mov al,0
    out dx,al
    inc dx

    mov si,offset palette
    mov cx,3*256						; Anzahl der Schleifendurchläufe
    rep outsb							; überträgt DS:SI zum durch DX adressierten Kanal
    pop ds
    ret
SetPal endp

